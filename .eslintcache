                if (data.resultCode === 0) {\r\n                    dispatch(setLogin(data.data.login, data.data.id, true));\r\n                } else {\r\n                    dispatch(setLogin(null, null, false));\r\n                }\r\n    }\r\n}\r\n\r\nexport const logIn = (email, password, rememberMe = false) => {\r\n    return async (dispatch) => {\r\n        let response = await authAPI.logIn(email, password, rememberMe)\r\n                if(response.data.resultCode === 0) {\r\n                    dispatch(setUser())\r\n                } else {\r\n                    let message = response.data.messages.length > 0 ? response.data.messages[0]: 'Some error';\r\n                    dispatch(stopSubmit('login', {_error: message}))\r\n                }\r\n    }\r\n}\r\n\r\nexport const logOut = () => {\r\n    return async (dispatch) => {\r\n        let response = await authAPI.logOut()\r\n                dispatch(setUser())\r\n    }\r\n}\r\n\r\nexport default loginReducer;","D:\\self-development\\programming\\web\\projects\\React-samurai\\01-react-samurai\\src\\redux\\app-reducer.js",[],"D:\\self-development\\programming\\web\\projects\\React-samurai\\01-react-samurai\\src\\selectors\\selectors.js",[],"D:\\self-development\\programming\\web\\projects\\React-samurai\\01-react-samurai\\src\\components\\Profile\\ProfileInfo\\ProfileStatusHocks.jsx",["184"],"import React, {useState, useEffect} from \"react\";\r\nimport {setStatusProfile} from \"../../../redux/profile-reducer\";\r\n\r\n\r\nlet ProfileStatus = (props) => {\r\n    let [editMode, setEditMode] = useState(false)\r\n    let [status, setStatus] = useState(props.status)\r\n\r\n    const activateEditMode = () => {\r\n        setEditMode(true)\r\n    }\r\n\r\n    const deactivateEditMode = () => {\r\n        setEditMode(false)\r\n        props.setStatus(status)\r\n    }\r\n\r\n    const onChangeStatus = (e) => {\r\n        setStatus(e.currentTarget.value)\r\n    }\r\n\r\n    useEffect(() => {\r\n        setStat